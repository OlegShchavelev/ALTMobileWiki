{
  "format": "plain",
  "checkYo": true,
  "fileExtensions": [".md"],
  "excludeFiles": ["docs/en"],
  "ignoreText": [
    ["```.+```", "gsm"],
    ["^  *", "gsm"],
    ["src:.*$", "gsm"],
    " ?`.+`",
    "`.+` ?",
    "#+ ",
    "\\- +",
    "\\*\\*.*\\*\\* ?",
    " ?\\*\\*.*\\*\\*",
    "!?\\[.+](.+)",
    "<!--@include",
    "https?:\/\/([^ ]+) ",
    "<Teams",
    "{{.*}}",
    "\\| +"
  ]
}
